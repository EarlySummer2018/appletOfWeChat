/**app.wxss**/

@import "./styles/iconfont.wxss";


page,view,text,swiper,swiper-item,image,navigator {
    padding: 0;
    margin: 0;
    box-sizing: border-box;
}


page {
    /* 定义主题颜色 */
    --themeColor: cyan;
    
    /* 定义统一字体大小
    
    1px = 2rpx

    14px = 28rpx
    
    */
    font-size: 28rpx;
}